{
  "name": "react-typescript",
  "version": "1.0.0",
  "type": "module",
  "description": "React TypeScript on Replit, using Vite bundler",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "npm run build && node server/server.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "test": "npm run build && jest --detectOpenHandles",
    "test:unit": "npm run build && jest --detectOpenHandles tests/unit",
    "test:integration": "npm run build && jest --detectOpenHandles tests/transcription.test.js",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vite": "^3.0.4"
  },
  "dependencies": {
    "assemblyai": "^4.7.0",
    "axios": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "glob": "^9.3.5",
    "multer": "^1.4.5-lts.1",
    "music-metadata": "^10.3.1",
    "openai": "^4.57.0"
  }
